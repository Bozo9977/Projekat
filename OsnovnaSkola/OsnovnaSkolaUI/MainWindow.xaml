<Window x:Class="OsnovnaSkolaUI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OsnovnaSkolaUI"
        mc:Ignorable="d"
        Title="MainWindow" WindowStartupLocation="CenterScreen"  Height="608.65" Width="918" WindowState="Maximized">
    <Grid Background="Black">
        <!--<Grid>-->
            <Label Content="Ime" FontWeight="Bold" HorizontalAlignment="Left" Margin="10,0,0,0" VerticalAlignment="Top" Foreground="gold"/>
            <TextBlock x:Name="Ime" HorizontalAlignment="Left" Margin="10,26,0,0" TextWrapping="Wrap" Text="{Binding LoggedIn.ime}" VerticalAlignment="Top" Width="124" Foreground="#FFFBFB9F"/>

            <Label Content="Prezime" FontWeight="Bold" HorizontalAlignment="Left" Margin="10,42,0,0" VerticalAlignment="Top" Foreground="gold"/>
            <TextBlock x:Name="Prezime" HorizontalAlignment="Left" Margin="10,68,0,0" TextWrapping="Wrap" Text="{Binding LoggedIn.prezime}" VerticalAlignment="Top" Width="124" Foreground="#FFFBFB9F"/>

            <Label Content="Zvanje"  FontWeight="Bold" HorizontalAlignment="Left" Margin="10,89,0,0" VerticalAlignment="Top" Foreground="gold" RenderTransformOrigin="0.405,-1.945"/>
            <TextBlock x:Name="Zvanje" HorizontalAlignment="Left" Margin="10,115,0,0" TextWrapping="Wrap" Text="{Binding LoggedIn.zvanje}" VerticalAlignment="Top" Width="124" Foreground="#FFFBFB9F"/>
            <!--<Button Content="Promeni" Command="{Binding ChangeInfoCommand}" Visibility="{Binding IsAdmin}" Background="Black" HorizontalAlignment="Center" Margin="10,136,1236,0" VerticalAlignment="Top" Width="107" FontSize="22" Foreground="#FFFBFB9F" BorderBrush="gold" Height="40"/>-->
            <!--<Button Content="Promeni Lozinku" Command="{Binding ChangePasswordCommand}"  Background="Black" HorizontalAlignment="Center" Margin="10,181,1177,0" VerticalAlignment="Top" Width="166" FontSize="22" Foreground="#FFFBFB9F" BorderBrush="gold" Height="40"/>-->
            
            <!--Meni opcije zaposleni-->
            <Menu HorizontalAlignment="Center" FontSize="16" Height="32"  VerticalAlignment="Top" Width="489" Visibility="{Binding AuthorizeZaposleni}">
                <MenuItem Header="Časovi" Height="32">
                    <MenuItem Header="Dodaj čas" Command="{Binding CreateCasCommand}"/>
                    <MenuItem Header="Evidentiraj prisustva" Command="{Binding EvidentirajPrisustvaCommand}"/>
                </MenuItem>
                <MenuItem Header="Kontrolne tačke">
                    <MenuItem Header="Dodaj domaći" Command="{Binding CreateDomaciCommand}"/>
                    <MenuItem Header="Dodaj kontrolni" Command="{Binding CreateKontrolniCommand}"/>
                </MenuItem>
                <MenuItem Header="Predavanja">
                    <MenuItem Header="Dodaj Predavanje" Command="{Binding CreatePredavanjeCommand}"/>
                    <MenuItem Header="Izmeni predavanje" Command="{Binding ChangePredavanjeCommand}"/>
                    <MenuItem Header="Obriši predavanje"  Command="{Binding DeletePredavanjeCommand}"/>
                </MenuItem>
                <MenuItem Header="Izveštaji/Raspored">
                    <MenuItem Header="Pošalji izveštaje o učenicima" Command="{Binding SendReportsCommand}"/>
                    <MenuItem Header="Prikaži raspored" Command="{Binding GetRasporedCommand}"/>
                </MenuItem>
            </Menu>
            <!--Meni promena info zaposleni-->
            <Menu HorizontalAlignment="Left" Height="28" Margin="10,131,0,0" VerticalAlignment="Top" Width="77" Visibility="{Binding AuthorizeZaposleni}">
                <MenuItem Header="Promeni" FontSize="16" >
                    <MenuItem Header="Lične informacije" Command="{Binding ChangeInfoCommand}" />
                    <MenuItem Header="Lozinku" Command="{Binding ChangePasswordCommand}"/>
                </MenuItem>
            </Menu>
            <!--Meni promena info admin-->
            <Menu HorizontalAlignment="Left" Height="28" Margin="10,131,0,0" VerticalAlignment="Top" Width="77" Visibility="{Binding AuthorizeAdmin}">
                <MenuItem Header="Promeni" FontSize="16" Height="28" Width="77" >
                    <MenuItem Header="Lozinku" Command="{Binding ChangePasswordCommand}"/>
                </MenuItem>
            </Menu>
            <!--Meni opcije admin-->
            <Menu HorizontalAlignment="Center" Visibility="{Binding AuthorizeAdmin}" FontSize="16" Height="31"  VerticalAlignment="Top" Width="381">
                <MenuItem Header="Zaposleni" Height="31">
                    <MenuItem Header="Dodaj zaposlenog" Command="{Binding AddZaposleniCommand}"/>
                    <MenuItem Header="Obriši zaposlenog" Command="{Binding DeleteZaposleniCommand}"/>
                </MenuItem>
                <MenuItem Header="Učenici">
                    <MenuItem Header="Dodaj učenika" Command="{Binding AddUcenikCommand}"/>
                    <MenuItem Header="Izmeni učenika" Command="{Binding ChangeUcenikCommand}"/>
                    <MenuItem Header="Obriši učenika" Command="{Binding DeleteUcenikCommand}"/>
                </MenuItem>
                <MenuItem Header="Odeljenja">
                    <MenuItem Header="Dodaj odeljenje" Command="{Binding AddOdeljenjeCommand}"/>
                    <MenuItem Header="Izmeni odeljenje"  Command="{Binding ChangeOdeljenjeCommand}"/>
                    <MenuItem Header="Obriši odeljenje" Command="{Binding DeleteOdeljenjeCommand}"/>
                </MenuItem>
                <MenuItem Header="Predmeti">
                    <MenuItem Header="Dodaj predmet" Command="{Binding AddPredmetCommand}"/>
                    <MenuItem Header="Izmeni predmet" Command="{Binding ChangePredmetCommand}"/>
                    <MenuItem Header="Obriši predmet" Command="{Binding DeletePredmetCommand}"/>
                </MenuItem>
                <MenuItem Header="Učionice">
                    <MenuItem Header="Dodaj učionicu" Command="{Binding AddUcionicaCommand}"/>
                    <MenuItem Header="Izmeni učionicu" Command="{Binding ChangeUcionicaCommand}"/>
                    <MenuItem Header="Obriši učionicu" Command="{Binding DeleteUcionicaCommand}"/>
                </MenuItem>
            </Menu>


